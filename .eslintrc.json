{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"rules": {
		"no-jquery/no-global-selector": "off",
		"no-jquery/no-class-state": "off",
		"no-jquery/no-fade": "off",
		"no-jquery/no-slide": "off",
		"no-jquery/no-sizzle": "off",
		"no-jquery/no-done-fail": "warn"
	}
}
